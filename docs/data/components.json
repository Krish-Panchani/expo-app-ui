    {
      "slug": "custom-text",
      "name": "Custom Text",
      "description": "A customizable Text component with font, color, and spacing props",
      "category": "components",
      "installation": "npx expo-app-ui add custom-text",
      "import": "import CustomText from \"@/components/ui/custom-text\";",
      "dependencies": ["theme"],
      "props": [
        {
          "name": "variant",
          "type": "'h1' | 'h2' | 'h3' | 'h4' | 'body' | 'caption'",
          "default": "'body'",
          "description": "Text variant"
        },
        {
          "name": "color",
          "type": "string",
          "default": "'primary'",
          "description": "Text color from theme"
        },
        {
          "name": "size",
          "type": "'xs' | 'sm' | 'md' | 'lg' | 'xl'",
          "default": "'md'",
          "description": "Text size"
        },
        {
          "name": "weight",
          "type": "'light' | 'regular' | 'medium' | 'semibold' | 'bold'",
          "default": "'regular'",
          "description": "Font weight"
        },
        {
          "name": "numberOfLines",
          "type": "number",
          "default": "undefined",
          "description": "Maximum number of lines"
        },
        {
          "name": "ellipsizeMode",
          "type": "'head' | 'middle' | 'tail' | 'clip'",
          "default": "'tail'",
          "description": "How to truncate text"
        }
      ],
      "examples": [
        {
          "title": "Basic Usage",
          "code": "<CustomText>Default text</CustomText>"
        },
        {
          "title": "Headings",
          "code": "<CustomText variant=\"h1\">Heading 1</CustomText>\n<CustomText variant=\"h2\">Heading 2</CustomText>"
        },
        {
          "title": "Colors",
          "code": "<CustomText color=\"primary\">Primary text</CustomText>\n<CustomText color=\"secondary\">Secondary text</CustomText>"
        }
      ],
      "features": [
        "Multiple text variants",
        "Customizable colors",
        "Font weight options",
        "Text truncation support"
      ]
    },
